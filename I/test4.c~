#include <stdio.h>

void myMode(const int *param1, const int param2, int *param3);

// ornek program: main4.c
int main ()
{
	int myArray[] = {2, 4, 4, 4, 5, 5, 7, 9};
	int result;

	myMode(myArray, 8, &result);

    printf("MOD: %d\n", result);

	return 0;
}

